class ListComparator:
  def _init_(self, list1, list2):
    self.list1=list1
    self.list2=list2

def calculate_average(self, num_list):
  if len(num_list) > 0:
    return sum(num_list) / len(num_list)
  else:
    return 0

def compare_lists(self):
  avg1=self.calculate_average(self.list1)
  avg2=self.calculate_average(self.list2)

if avg1>avg2:
 return "Average of List1 is greater then Average of List2"
elif avg1<avg2:
  return "Average of List1 is less then Average of List2"
else
  return "Average of List1 is equal to Average of List2"


